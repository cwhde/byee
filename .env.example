# Byee Environment Configuration
# Copy this file to .env and customize

# REQUIRED: Public URL of your instance
# This URL is embedded in installer and client scripts
BYEE_PUBLIC_URL=https://byee.example.com

# Storage path for encrypted files (default: ./data)
# BYEE_STORAGE_PATH=./data

# Maximum file size in bytes (default: 100GB = 107374182400)
# BYEE_MAX_FILE_SIZE=107374182400

# Number of words in generated file IDs (default: 4)
# BYEE_ID_WORD_COUNT=4
